{
  "name": "scheduler-service",
  "version": "0.3.2",
  "description": "Scheduler for sammler.",
  "keywords": [
    "sammler"
  ],
  "homepage": "https://github.com/sammler/scheduler-service",
  "bugs": {
    "url": "https://github.com/sammler/scheduler-service/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Stefan Walther",
    "url": "https://github.com/stefanwalther",
    "twitter": "waltherstefan"
  },
  "main": "/src/index.js",
  "repository": "sammler/scheduler-service",
  "scripts": {
    "coverage": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "d-build": "docker build -t local/scheduler-service .",
    "dc-dev-down": "docker-compose --f=./docker-compose.dev.yml down",
    "dc-dev-up": "docker-compose --f=./docker-compose.dev.yml up --build",
    "docs": "verb",
    "lint": "yarn run lint:src && yarn run lint:test",
    "lint:fix": "yarn run lint:src:fix && yarn run lint:test:fix",
    "lint:src": "eslint src",
    "lint:src:fix": "eslint src --fix",
    "lint:test": "eslint test",
    "lint:test:fix": "eslint test --fix",
    "start": "node ./src/index.js",
    "start:dev": "nodemon -L --watch ./src --exec yarn run start",
    "test": "yarn run test:unit && yarn run test:integration",
    "test:integration": "yarn run lint:test && mocha './test/integration/**/*.spec.js' --require './test/mocha.conf.js'",
    "test:unit": "yarn run lint:test && mocha './test/unit/**/*.spec.js' --require './test/mocha.conf.js'"
  },
  "dependencies": {
    "amqp-winston": "^1.0.7",
    "amqplib": "^0.5.1",
    "amqplib-sugar": "sammler/amqplib-sugar",
    "lodash": "^4.17.4",
    "node-schedule": "^1.2.0",
    "sammler-nodelib-logger": "^0.2.1",
    "superagent": "^3.4.1",
    "superagent-promise": "^1.1.0",
    "winster": "^0.2.5"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.14.1",
    "eslint-config-sammler": "sammler/eslint-config-sammler",
    "eslint-plugin-mocha": "^4.8.0",
    "gulp-format-md": "*",
    "mocha": "*",
    "nyc": "^10.1.2",
    "sammler-base": "sammler/sammler-base"
  },
  "engines": {
    "node": ">=7.2"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": []
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb",
      "verb-readme-generator"
    ]
  },
  "winster": {
    "configFile": "./src/helper/winster-configuration.js"
  }
}
